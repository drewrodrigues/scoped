{
  "name": "scoped",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "build:client": "webpack -c client-webpack.config.js",
    "build:desktop": "tsc --project desktop-tsconfig.json",
    "build": "concurrently -n client,desktop -c 'bgGreen.bold,bgBlue.bold' 'yarn build:client' 'yarn build:desktop'",
    "build:watch": "concurrently -n client,desktop -c 'bgGreen.bold,bgBlue.bold' 'yarn build:client -w' 'yarn build:desktop -w'",
    "start": "concurrently -n client,desktop-watch,desktop,styles -c 'bgGreen.bold,bgBlue.bold,bgYellow.bold,bgRed.bold' 'yarn serve:client' 'yarn build:desktop -w' 'yarn start:desktop' 'yarn styles'",
    "serve:client": "webpack serve -c client-webpack.config.js --progress",
    "start:desktop": "nodemon --exec electron dist/desktop/desktop.js --inspect",
    "styles": "npx tailwind -i ./client/src/styles/styles.css -o ./client/src/styles/output.css --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "private": "true",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@types/cleave.js": "^1.4.6",
    "@types/pouchdb": "6.4.0",
    "@types/pouchdb-find": "6.3.7",
    "@types/react-dom": "^17.0.11",
    "@types/react-router": "5.1.17",
    "@types/react-router-dom": "5.3.2",
    "babel-loader": "^8.2.3",
    "concurrently": "6.4.0",
    "css-loader": "^6.5.1",
    "electron": "^16.0.4",
    "electron-devtools-installer": "^3.2.0",
    "electron-redux-devtools": "^0.3.4",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.15",
    "react": "^17.0.2",
    "sass": "^1.44.0",
    "sass-loader": "^12.3.0",
    "storybook": "^6.4.9",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.9",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "cleave.js": "^1.6.0",
    "moment": "^2.29.1",
    "postcss": "^8.4.5",
    "pouchdb": "^7.2.2",
    "pouchdb-debug": "^7.2.1",
    "pouchdb-find": "^7.2.2",
    "react-dom": "^17.0.2",
    "react-icons": "4.3.1",
    "react-redux": "^7.2.6",
    "react-router": "5.2.1",
    "react-router-dom": "5.3.0"
  }
}
